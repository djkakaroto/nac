      <tuv lang="EN-US">
        <seg>&lt;f0&gt;network_detection_ip&lt;/f0&gt; under &lt;f1&gt;[captive_portal]&lt;/f1&gt;</seg>
      </tuv>
      <tuv lang="PT-BR" changedate="20110814T015439Z" changeid="djkakaroto">
        <seg>&lt;f0&gt;network_detection_ip&lt;/f0&gt; em &lt;f1&gt;[captive_portal]&lt;/f1&gt;</seg>
      </tuv>
    </tu>
